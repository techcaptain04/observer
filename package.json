{
  "name": "my-monorepo",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "start-frontend": "npm run start --prefix frontend",
    "start-backend": "npm run start:dev --prefix screenshot_backend",
    "dev": "concurrently \"npm run start-frontend\" \"npm run start-backend\""
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "install": "^0.13.0",
    "npm": "^11.8.0"
  }
}
